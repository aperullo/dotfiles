{
    "workbench.colorTheme": "Mariana Nord",
    "debug.console.fontSize": 16,
    "editor.tokenColorCustomizations": {
        "[Mariana Nord]": {
            "textMateRules": [
                {
                    "scope": "string.quoted.docstring.multi.python",
                    "settings": {
                        "foreground": "#A6ACB9"
                    }
                }
            ]
        }
    },
    "editor.acceptSuggestionOnEnter": "on",
    "editor.codeActionsOnSave": null,
    "editor.bracketPairColorization.enabled": false,
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "editor.inlineSuggest.enabled": true,
    "editor.fontSize": 16,
    "jupyter.askForKernelRestart": false,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false,
        "python": true
    },
    "go.formatTool": "goimports",
    "[go]": {
        "editor.insertSpaces": false,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "python.pythonPath": "/usr/bin/python",
    "python.linting.enabled": true,
    "python.linting.pylintPath": "/usr/local/bin/pylint",
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--disable",
        "trailing-whitespace,missing-function-docstring,missing-class-docstring,fixme,line-too-long,bad-indentation"
    ],
    "python.linting.pylintUseMinimalCheckers": false,
    "python.formatting.autopep8Path": "/usr/local/bin/autopep8",
    "python.formatting.autopep8Args": [
        "--max-line-length",
        "120"
    ],
    "python.languageServer": "Pylance",
    "python.analysis.extraPaths": ["/usr/bin/python3.8"],
    "python.analysis.typeCheckingMode": "basic",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "window.titleBarStyle": "custom",
    "vsintellicode.java.completionsEnabled": false,
    "vsintellicode.sql.completionsEnabled": false,
    "vsintellicode.features.python.deepLearning": "enabled",
    "jupyter.interactiveWindowMode": "perFile",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    }
}